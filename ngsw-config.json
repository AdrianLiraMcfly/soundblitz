{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/icons/**",
          "/*.png",
          "/*.jpg",
          "/*.jpeg",
          "/*.svg"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-data",
      "urls": [
        "http://localhost:8085/api/**"
      ],
      "cacheConfig": {
        "maxSize": 200,
        "maxAge": "7d",
        "timeout": "5s",
        "strategy": "performance"
      },
      "cacheQueryOptions": {
        "ignoreSearch": false
      }
    },
    {
      "name": "archivos-multimedia",
      "urls": [
        "http://localhost:8085/files/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "30d",
        "strategy": "performance"
      }
    }
  ]
}